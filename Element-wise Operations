# Create another array for operations
data2 = np.array([2, 3, 2, 3, 2, 3, 2, 3, 2, 3])

# Element-wise addition
add_result = data + data2
print("Addition result:", add_result)

# Element-wise multiplication
mult_result = data * data2
print("Multiplication result:", mult_result)

# Element-wise square root
sqrt_result = np.sqrt(data)
print("Square roots:", sqrt_result)

# Element-wise exponential
exp_result = np.exp(data)
print("Exponentials:", exp_result)
